{
  "name": "explorer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "animate-components": "^1.4.8",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "classnames": "^2.2.6",
    "events": "^3.3.0",
    "highcharts": "^9.0.0",
    "highcharts-react-official": "^2.2.2",
    "immutability-helper": "^2.7.1",
    "jszip": "^3.9.1",
    "jwt-decode": "^3.1.2",
    "lint-staged": "^10.5.4",
    "prismjs": "^1.24.0",
    "react": "^16.14.0",
    "react-bootstrap": "^0.32.3",
    "react-bootstrap-table-next": "^1.1.3",
    "react-dom": "^16.14.0",
    "react-easy-state": "^5.1.0",
    "react-octicons": "^0.2.0",
    "react-prism": "^4.3.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.4",
    "react-select": "^5.2.2",
    "react-spinners": "^0.4.5",
    "react-table": "^7.7.0",
    "react-tooltip": "^4.2.14",
    "reactstrap": "^8.9.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "semver": "^7.3.7",
    "semaphore-async-await": "^1.5.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "start:backend": "node backend-src/server.js",
    "start:backend-dev": "$(npm bin)/nodemon backend-src/server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --collectCoverage --testPathIgnorePatterns end2end",
    "testE2E": "react-scripts test --env=jsdom end2end",
    "eject": "react-scripts eject",
    "docker:build": "docker build . -t registry.aura.rest/explorer",
    "docker:push": "docker push registry.aura.rest/explorer",
    "docker:deploy": "docker build . -t registry.aura.rest/explorer && docker push registry.aura.rest/explorer"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{js}",
      "src/routes/**/*.{js}",
      "src/services/**/*.{js}",
      "src/App.js",
      "src/AppContent.js"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.5",
    "ansi-styles": "^3.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "husky": "^4.3.0",
    "nodemon": "^1.19.4",
    "prettier": "^2.5.1",
    "puppeteer": "^2.1.1",
    "react-shallow-renderer": "^16.14.1"
  },
  "nodemonConfig": {
    "ignore": [
      "config/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
