    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "jest": {
        "collectCoverageFrom": [
            "src/components/**/*.{js}",
            "src/routes/**/*.{js}",
            "src/services/**/*.{js}",
            "src/App.js",
            "src/AppContent.js"
